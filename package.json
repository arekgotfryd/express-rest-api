{
  "name": "express-rest-api",
  "version": "1.0.0",
  "description": "A RESTful API built with Express and Sequelize",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "vite build",
    "build:tsc": "tsc",
    "test": "vitest run tests/unit",
    "test:e2e": "vitest run tests/e2e",
    "test:watch": "vitest",
    "test:coverage": "vitest run tests/unit --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:seed": "tsx src/db/seed.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "api",
    "nodejs",
    "express",
    "sequelize"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@epic-web/remember": "^1.1.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "custom-env": "^2.0.6",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "figlet": "^1.10.0",
    "helmet": "^7.1.0",
    "jose": "^6.0.12",
    "lru-cache": "^11.2.4",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.3",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  }
}
